{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "test": {
      "dependsOn": ["^test"],
      "outputs": ["coverage/**", "vitest-report.json"],
      "cache": true
    },
    "test:api": {
      "outputs": ["coverage/**", "vitest-report.json"],
      "cache": true
    },
    "test:web": {
      "outputs": ["coverage/**", "vitest-report.json"],
      "cache": true
    },
    "lint": {
      "outputs": []
    }
  }
}
